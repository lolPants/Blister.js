{
  "name": "blister.js",
  "version": "0.2.0",
  "description": "Beat Saber playlist handling Library, now with more JavaScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "beat",
    "saber",
    "vr",
    "playlist",
    "serialize",
    "deserialize"
  ],
  "scripts": {
    "prepublishOnly": "yarn run test && yarn run build",
    "test": "yarn run check && yarn run lint",
    "check": "yarn run build --noEmit",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "yarn run watch  --sourceMap"
  },
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "repository": "git@github.com:lolPants/Blister.js.git",
  "license": "MIT",
  "devDependencies": {
    "@types/bson": "^4.0.0",
    "@types/node": "^12.7.4",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "bson": "^4.0.2"
  }
}
